#include <stdio.h>

int main() {
    int pageFrames[20], pages[50];
    int frameCount, pageCount;
    int i, j, k, pageFaults = 0, nextToReplace = 0, found;

    printf("Enter number of page frames: ");
    scanf("%d", &frameCount);

    printf("Enter number of pages: ");
    scanf("%d", &pageCount);

    printf("Enter the page reference string:\n");
    for (i = 0; i < pageCount; i++)
        scanf("%d", &pages[i]);

    
    for (i = 0; i < frameCount; i++)
        pageFrames[i] = -1;

    printf("\nPage Reference\tFrames\t\tPage Fault\n");

    for (i = 0; i < pageCount; i++) {
        found = 0;

        
        for (j = 0; j < frameCount; j++) {
            if (pageFrames[j] == pages[i]) {
                found = 1;
                break;
            }
        }

        
        if (found == 0) {
            pageFrames[nextToReplace] = pages[i];
            nextToReplace = (nextToReplace + 1) % frameCount;
            pageFaults++;
        }

        
        printf("%d\t\t", pages[i]);
        for (k = 0; k < frameCount; k++) {
            if (pageFrames[k] != -1)
                printf("%d ", pageFrames[k]);
            else
                printf("- ");
        }

        if (found == 0)
            printf("\t\tYes");
        else
            printf("\t\tNo");

        printf("\n");
    }

    printf("\nTotal Page Faults = %d\n", pageFaults);

    return 0;
}
